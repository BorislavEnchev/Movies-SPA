<div class="movie">
    <h2>Title: {{title}}</h2>
    <img src="{{imageUrl}}">
    <p id="availableTickets">Available Tickets: {{tickets}}</p>
    <div class="btn-group">
        <button><a href="#/edit/{{id}}">Edit</a></button>
        <button><a href="#/delete/{{id}}">Delete</a></button>
        <button><a href="#/buy/{{id}}?origin=myMovies">Buy Ticket</a></button>
        <button><a href="#/details/{{id}}">Details</a></button>
    </div>
</div>